
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterPlantilla.cshtml";
}

<h2>View</h2>
<div class="row">

    <div class="col-lg-12">

        <div class="portlet portlet-default">
            <div class="portlet-heading">
                <div class="portlet-title">
                    <h4>Max Character Inputs</h4>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="portlet-body">
                <h4>Normal Input</h4>
                <input type="text" class="form-control" maxlength="50" id="basicMax" />
                <h4>Text Area</h4>
                <textarea id="textareaMax" class="form-control" maxlength="225" rows="2" placeholder="Limit 225 characters."></textarea>
                <h4>Threshold</h4>
                <input type="text" class="form-control" maxlength="25" id="thresholdMax" placeholder="Start typing..." />
            </div>
        </div>
        </div>
    </div>

<script src="js/plugins/bootstrap-tokenfield/bootstrap-tokenfield.min.js"></script>
<script src="js/plugins/bootstrap-tokenfield/scrollspy.js"></script>
<script src="js/plugins/bootstrap-tokenfield/affix.js"></script>
<script src="js/plugins/bootstrap-tokenfield/typeahead.min.js"></script>
<script src="js/plugins/bootstrap-maxlength/bootstrap-maxlength.js"></script>
<script src="js/plugins/bootstrap-timepicker/bootstrap-timepicker.min.js"></script>
<script src="js/plugins/bootstrap-datepicker/bootstrap-datepicker.js"></script>

        @section Widgets{
            <li class="panel">
                <a href="javascript:;" data-parent="#side" data-toggle="collapse" class="accordion-toggle collapsed" data-target="#aptitudes">
                    <i class="fa fa-edit"></i>Reportes de Examen de Aptitudes<i class="fa fa-caret-down"></i>
                </a>
                <ul class="collapse nav" id="aptitudes">
                    <li>
                        <a href="@Url.Action("aprobacionAptitudes")">
                            <i class="fa fa-angle-double-right"></i> Reportes de Aprobación
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("calificacionesAptitudes")">
                            <i class="fa fa-angle-double-right"></i> Reporte de Calificaciones
                        </a>
                    </li>
                </ul>
            </li>
            <li class="panel">
                <a href="javascript:;" data-parent="#side" data-toggle="collapse" class="accordion-toggle collapsed" data-target="#postulacion">
                    <i class="fa fa-edit"></i>Reportes de Examen de Postulación<i class="fa fa-caret-down"></i>
                </a>
                <ul class="collapse nav" id="postulacion">
                    <li>
                        <a href="@Url.Action("aprobacionPostulacion")">
                            <i class="fa fa-angle-double-right"></i> Reportes de Aprobación
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("calificacionesPostulacion")">
                            <i class="fa fa-angle-double-right"></i> Reporte de Calificaciones
                        </a>
                    </li>
                </ul>
            </li>
            <li class="panel">
                <a href="javascript:;" data-parent="#side" data-toggle="collapse" class="accordion-toggle" data-target="#modulos">
                    <i class="fa fa-edit"></i>Reportes de Módulos<i class="fa fa-caret-down"></i>
                </a>
                <ul class="collapse nav in" id="modulos">
                    <li>
                        <a href="@Url.Action("aprobacionModulos")">
                            <i class="fa fa-angle-double-right"></i> Reportes de Aprobación
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("calificacionesModulos")">
                            <i class="fa fa-angle-double-right"></i> Reporte de Calificaciones
                        </a>
                    </li>
                </ul>
            </li>
        }
